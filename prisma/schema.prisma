// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Appointment {
  id Int @id
  patientId String
  date String
  type String
  patient Patient @relation(fields: [patientId], references: [id])
}

model Patient {
    id String @id
    firstName String
    lastName String
    name String
    dob String
    email String
    phone String
    address String
    appointments Appointment[]
}